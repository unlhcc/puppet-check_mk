# puppet maintained

<% if !@check_mk_tags.empty? i-%>
<% if @check_mk_tags.is_a?(Array) -%>
all_hosts = all_hosts + [ "<%= @fqdn %>|<%= @check_mk_tags.join("|") %>" ]
<% else %>
all_hosts = all_hosts + [ "<%= @fqdn %>|<%= @check_mk_tags %>" ]
<% end -%>
<% else %>
all_hosts = all_hosts + [ "<%= @fqdn %>" ]
<% end -%>
